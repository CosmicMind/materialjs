{
  "name": "@cosmicverse/material",
  "version": "0.0.1-rc-11722-1",
  "description": "A Material Design framework in TypeScript.",
  "keywords": [],
  "author": {
    "name": "Daniel Jonathan",
    "email": "daniel@cosmicverse.org"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmicverse/material.git"
  },
  "type": "module",
  "main": "dist/lib.es.js",
  "module": "dist/lib.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint:exec": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "tests:build": "vite build --config vite.config.tests.js --mode development",
    "tests:watch": "vite build --config vite.config.tests.js --mode watch",
    "tests:exec": "c8 ava dist/lib.es.tests.js",
    "dev:build": "vite build --config vite.config.lib.js --mode development",
    "dev:watch:lib": "vite build --config vite.config.lib.js --mode watch",
    "prod:build:lib": "vite build --config vite.config.lib.js --mode production",
    "prod:build": "npm run prod:build:lib",
    "release:build:lib": "npm run prod:build:lib",
    "release:build": "npm run prod:build",
    "package:publish": "npm publish",
    "test": "npm run tests:build; npm run tests:exec"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "ava": "^4.3.3",
    "c8": "^7.12.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.4.0",
    "eslint-plugin-yml": "^1.1.0",
    "nodemon": "^2.0.19",
    "sass": "^1.54.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
